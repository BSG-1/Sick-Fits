type User {
  id: ID! @unique
  name: String!
  email: String!
}

# Anytime you change anything in your data-model, you must deploy it to prisma 
type Item {
  id: ID! @unique
  title: String!
  description: String!
  image: String
  largeImage: String
  price: Int!
  createAt: DateTime!
  updatedAt: DateTime!
  # user: User!
}